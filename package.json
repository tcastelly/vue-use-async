{
  "name": "vue-use-async",
  "version": "0.2.13",
  "description": "Xhr and async helpers",
  "repository": {
    "type": "git",
    "url": "https://github.com/tcastelly/vue-use-async.git"
  },
  "license": "MIT",
  "author": "Thomas CASTELLY",
  "main": "dist/index.js",
  "scripts": {
    "afterbuild": "node ./scripts/afterbuild.js",
    "build": "npm run build:webpack && npm run afterbuild",
    "build:webpack": "webpack --mode=production",
    "flow": "flow",
    "flow:deps": "flow-typed install jest@22.x",
    "lint": "eslint src tests",
    "lint:fix": "eslint --fix src tests",
    "precommit": "lint-staged",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "test": "jest tests/** --no-cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@vue/composition-api": "^0.6.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-flowtype-errors": "^4.3.0",
    "eslint-plugin-import": "^2.22.0",
    "flow-bin": "^0.133.0",
    "flow-typed": "^3.2.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.3.0",
    "vue": "^2.6.12",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.6.7",
    "vue": "^2.6.12"
  },
  "engines": {
    "node": ">=10"
  },
  "os": [
    "darwin",
    "linux"
  ]
}
